<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_marginLeft="10dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="@dimen/bite_size"
        android:id="@+id/mine_item_tv_name"
        android:text="111"
        android:layout_margin="@dimen/mine_margin"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/mine_item_tv_name"
        android:textSize="@dimen/bite_size"
        android:id="@+id/mine_item_tv_thing"
        android:text="111"
        android:layout_marginLeft="@dimen/mine_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/mine_item_tv_thing"
        android:textSize="@dimen/bite_size"
        android:id="@+id/mine_item_tv_jine"
        android:text="金额："
        android:layout_margin="@dimen/mine_margin"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_toRightOf="@id/mine_item_tv_jine"
        android:layout_below="@id/mine_item_tv_thing"
        android:textSize="@dimen/bite_size"
        android:id="@+id/mine_item_tv_money"
        android:text="111"
        android:layout_marginTop="@dimen/mine_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/mine_item_tv_jine"
        android:textSize="@dimen/bite_size"
        android:id="@+id/mine_item_tv_time"
        android:text="111"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/mine_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/mine_item_tv_time"
        android:layout_width="80dp"
        android:layout_height="10dp" />
    <RelativeLayout
        android:id="@+id/mine_item_rl"
        android:layout_marginLeft="@dimen/mine_margin"
        android:layout_marginTop="3dp"
        android:background="@drawable/mine_type_shape"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_marginLeft="@dimen/login_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
            android:id="@+id/mine_item_tv_type"
            android:text="333333"
            android:layout_marginTop="7dp"
            android:textSize="@dimen/bite_size"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
            <TextView
                android:layout_below="@id/mine_item_tv_type"
                android:layout_width="30dp"
                android:layout_height="7dp" />
        </RelativeLayout>
    </RelativeLayout>
    <TextView
        android:text="444"
        android:id="@+id/mine_item_tv_price"
        android:textSize="@dimen/bite_size"
        android:layout_alignParentRight="true"
        android:layout_below="@id/mine_item_rl"
        android:layout_marginTop="@dimen/login_margin"
        android:layout_marginRight="@dimen/mine_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/mine_item_tv_leader"
        android:layout_below="@id/mine_item_tv_jine"
        android:text="55555"
        android:textSize="@dimen/bite_size"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginLeft="@dimen/mine_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:id="@+id/mine_item_iv_leader"
        android:layout_toRightOf="@id/mine_item_tv_leader"
        android:layout_below="@id/mine_item_tv_jine"
        android:layout_marginLeft="5dp"
        android:layout_width="20dp"
        android:layout_height="20dp" />
</RelativeLayout>
</RelativeLayout>