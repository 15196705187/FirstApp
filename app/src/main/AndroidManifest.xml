<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="trypost.dafeng.com.trypost">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo03"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomeActivity" android:launchMode="singleTop"/>
        <activity android:name=".activity.MineActivity"/>
        <activity android:name=".activity.ApplyReimburseActivity"/>
        <activity android:name=".activity.ReimburseItemActivity"/>
    </application>
</manifest>